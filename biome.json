{
	"$schema": "https://biomejs.dev/schemas/1.3.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		},
		"ignore":["build"]
	},
	"formatter":{
		"enabled": true,
		"lineWidth": 120,
		"formatWithErrors": true,
		"indentStyle": "space"
	}
}
